        "ggqZmhPjorv_text": "<span style=\"font-size:30px;\"><strong>La recherche&nbsp;scientifique&nbsp;</strong></span><br><span style=\"font-size:30px;\"><strong>révèle&nbsp;la véritable cause&nbsp;de la perte de cheveux</strong></span>"
      },
